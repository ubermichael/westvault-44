<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>PKP PLN Terms of Use</title>
    <subtitle>The terms of service for the PKP PLN.</subtitle>
    <link href="{{ url('feed_terms', {'_format': 'atom'}) }}" rel="self" />
    <id>{{ url('feed_terms', {'_format': 'atom'}) }}</id>
    <updated>{{terms[0].updated|date('Y-m-d\TH:i:sP')}}</updated>
    {% for term in terms %}
    <entry xml:lang="{{ term.langCode }}">
        <title>Term updated</title>
        <link href="{{ url('term_of_use_show', {'id': term.id}) }}" />
        <id>{{ term.keyCode }}</id>
        <updated>{{ term.updated|date('Y-m-d\TH:i:sP') }}</updated>
        <content type="text">
            {{term.content }}
        </content>
    </entry>
    {% endfor %}
</feed>
