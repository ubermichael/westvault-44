<table class="table table-bordered table-condensed table-hover table-striped">
    <thead>
        <tr>
            <th>Uuid</th>
            <th>Comment</th>
        </tr>
    </thead>
    <tbody>
        {% for whitelist in whitelists %}
            <tr>
                <td>
                    <a href="{{ path('whitelist_show', { 'id': whitelist.id }) }}">
                        {{ whitelist.uuid }}
                    </a>
                </td>
                <td>
                    {{ whitelist.comment|raw }}
                </td>
            </tr>
        {% endfor %}
    </tbody>
</table>
