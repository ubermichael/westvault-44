<table class='table table-bordered table-condensed table-hover table-striped'>
    <tbody>
        <tr>
            <th>Uuid</th>
            <td>
                {{ whitelist.uuid }}
            </td>
        </tr>
        <tr>
            <th>Comment</th>
            <td>
                {{ whitelist.comment|raw }}
            </td>
        </tr>
        <tr>
            <th>Provider</th>
            <td>
                {% if provider %}
                    <a href='{{ path('provider_show', {'id': provider.id}) }}'>{{ provider }}</a>
                {% endif %}
            </td>
        </tr>
        <tr>
            <th>Created</th>
            <td>{{ whitelist.created|date }}</td>
        </tr>
        <tr>
            <th>Updated</th>
            <td>{{ whitelist.updated|date }}</td>
        </tr>
    </tbody>
</table>


